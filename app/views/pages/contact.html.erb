<!-- Page title Section-->
<div class="page-title">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <h2>Contact</h2>
            </div>
        </div>
    </div>
</div>
<div class="clearfix"></div>

<!-- Contact Section -->
<section class="contact-details">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="contact-address">

                    <h3 class="title">Contact Address</h3>
                    <ul>
                        <li><a href="tel: +33 6 27 92 23 93"><i class="fa fa-phone" aria-hidden="true"></i> &nbsp; +33 6 27 92 23 93</a></li>
                        <li><a href="mailto: contact@securitykeys.eu"><i class="far fa-envelope" aria-hidden="true"></i> &nbsp; contact@securitykeys.eu</a></li>
                    </ul>
                </div>
            </div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                <!-- Contact Form Section -->
                <div class="contact-form">

                    <h3 class="title">Contact Form</h3>

                    <form name="form" class="form" method="post" action="#" enctype="multipart/form-data">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6 col-sm-12 col-xs-12">
                                    <input class="form-control" name="name" type="text" placeholder="Enter Your Name" />
                                </div>
                                <div class="col-md-6 col-sm-12 col-xs-12">
                                    <input class="form-control" name="address" type="text" placeholder="Enter Your Address" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6 col-sm-12 col-xs-12">
                                    <input class="form-control" name="email" type="email" placeholder="Enter Your Email Address" />
                                </div>
                                <div class="col-md-6 col-sm-12 col-xs-12">
                                    <input class="form-control" name="phone" type="text" placeholder="Enter Telephone Number" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <textarea class="form-control" name="message" placeholder="Enter Your Message"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <input name="submit" class="more viewmore-3" type="submit" value="Submit" />
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>